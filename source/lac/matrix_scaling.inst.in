for (S : REAL_SCALARS)
  {
    template bool MatrixScaling::find_scaling_and_scale_matrix(
      SparseMatrix<S> &);
    template bool MatrixScaling::find_scaling_and_scale_linear_system(
      SparseMatrix<S> &, Vector<S> &);

    template bool MatrixScaling::find_scaling_and_scale_matrix(
      SparseMatrixEZ<S> &);
    template bool MatrixScaling::find_scaling_and_scale_linear_system(
      SparseMatrixEZ<S> &, Vector<S> &);

    template bool MatrixScaling::find_scaling_and_scale_matrix(
      BlockSparseMatrix<S> &);
    template bool MatrixScaling::find_scaling_and_scale_linear_system(
      BlockSparseMatrix<S> &, BlockVector<S> &);

    template bool MatrixScaling::find_scaling_and_scale_matrix(
      BlockSparseMatrixEZ<S> &);
    template bool MatrixScaling::find_scaling_and_scale_linear_system(
      BlockSparseMatrixEZ<S> &, BlockVector<S> &);

    template bool MatrixScaling::find_scaling_and_scale_matrix(FullMatrix<S> &);
    template bool MatrixScaling::find_scaling_and_scale_linear_system(
      FullMatrix<S> &, Vector<S> &);

    template void MatrixScaling::scale_system_solution(Vector<S> &) const;
    template void MatrixScaling::scale_system_solution(BlockVector<S> &) const;
  }
